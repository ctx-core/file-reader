{
	"name": "@ctx-core/file-reader",
	"version": "7.2.11",
	"description": "ctx-core file-reader",
	"keywords": [
		"ctx-core",
		"FileReader",
		"file-reader"
	],
	"homepage": "https://github.com/ctx-core/file-reader#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/file-reader/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/file-reader.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-file-reader@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./readAs",
		"./readAs_ArrayBuffer",
		"./readAs_DataURL",
		"./readAs_Text"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.9.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}